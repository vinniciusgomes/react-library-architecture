{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "linterOptions": {
    "exclude": ["**/node_modules/**/*"]
  },
  "jsRules": {},
  "rules": {
    "prettier": true,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "interface-name": false
  },
  "rulesDirectory": []
}
